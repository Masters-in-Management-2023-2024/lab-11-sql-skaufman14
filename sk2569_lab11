      "cell_type": "code",
      "source": [
        "result = ctx.execute(\"\"\"\n",
        "    SELECT\n",
        "        f.carrier,\n",
        "        AVG(2013 - p.year) AS avg_age\n",
        "    FROM flights AS f\n",
        "    JOIN planes  AS p\n",
        "      ON f.tailnum = p.tailnum\n",
        "    WHERE p.year IS NOT NULL AND p.year <= 2013\n",
        "    GROUP BY f.carrier\n",
        "    ORDER BY avg_age DESC\n",
        "\"\"\")\n",
        "print(result)"
