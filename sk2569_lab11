        "outputId": "18b5060a-a715-4b1a-a360-d01a7a864038"
      },
      "source": [
        "result = ctx.execute(\"\"\"\n",
        "    SELECT\n",
        "        origin,\n",
        "        origin_name,\n",
        "        dest,\n",
        "        dest_name,\n",
        "        COUNT(*) AS n_flights,\n",
        "        AVG(dep_delay) AS avg_dep_delay,\n",
        "        100.0 * SUM(CASE WHEN dep_delay > 30 THEN 1 ELSE 0 END) / COUNT(*) AS pct_delay_gt_30\n",
        "    FROM (\n",
        "        SELECT\n",
        "            f.origin,\n",
        "            ao.name AS origin_name,\n",
        "            f.dest,\n",
        "            ad.name AS dest_name,\n",
        "            f.dep_delay\n",
        "        FROM flights AS f\n",
        "        LEFT JOIN airports AS ao\n",
        "          ON f.origin = ao.faa\n",
        "        LEFT JOIN airports AS ad\n",
        "          ON f.dest = ad.faa\n",
        "    ) AS subquery\n",
        "    GROUP BY\n",
        "        origin, origin_name, dest, dest_name\n",
        "    ORDER BY n_flights DESC\n",
        "    LIMIT 10\n",
        "\"\"\")\n",
        "print(result)"
      ],
