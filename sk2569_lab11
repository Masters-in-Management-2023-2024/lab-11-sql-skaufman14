      "cell_type": "code",
      "source": [
        "result = ctx.execute(\"\"\"\n",
        "    SELECT\n",
        "        p.manufacturer,\n",
        "        p.model,\n",
        "        COUNT(*) AS n_flights\n",
        "    FROM flights AS f\n",
        "    JOIN planes  AS p\n",
        "      ON f.tailnum = p.tailnum\n",
        "    GROUP BY p.manufacturer, p.model\n",
        "    ORDER BY n_flights DESC\n",
        "    LIMIT 10\n",
        "\"\"\")\n",
        "print(result)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "JMNAbryyC4Oa",
