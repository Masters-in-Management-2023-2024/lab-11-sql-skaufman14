        "polars_top_dest = (\n",
        "    flights\n",
        "    .group_by(\"dest\")\n",
        "    .agg(pl.len().alias(\"n_flights\"))\n",
        "    .sort(\"n_flights\", descending=True)\n",
        "    .limit(10)\n",
        ")\n",
        "print(polars_top_dest)"
